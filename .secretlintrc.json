{
  "rules": [
    {
      "id": "@secretlint/secretlint-rule-preset-recommend"
    }
  ],
  "ignorePatterns": [
    "**/node_modules/**",
    "**/.next/**",
    "**/out/**",
    "**/build/**",
    "**/.vercel/**",
    "**/*.png",
    "**/*.jpg",
    "**/*.jpeg",
    "**/*.webp",
    "**/*.ico",
    "**/*.zip",
    "**/*.pdf",
    "**/*.docx",
    "**/*.sql",
    "package-lock.json",
    "**/DATABASE_SETUP.md",
    "**/FRESH_DATABASE_SETUP.md",
    ".env.example",
    ".env.example.bak"
  ]
}

